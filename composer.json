    "minimum-stability": "dev",
    "authors":[
        {
            "name":"Svyatoslav Zhylichev",
            "email":"tomfordrumm@gmail.com"
        },
        {
            "name":"Sergey Petrov",
            "email":"sergey.petrov@snowcommerce.com"
        }
    ],
    "require":{
        "magento-hackathon/magento-composer-installer":"*"
