{
    "name":"slv/upsmi",
    "type":"magento-module",
    "homepage":"https://github.com/sergoslav/uspmi.git",
    "description":"UPSMI Magento extension",
    "minimum-stability": "dev",
    "authors":[
        {
            "name":"Sergey Petrov",
            "email":"sergoslav@gmail.com"
        }
    ],
    "require":{
        "magento-hackathon/magento-composer-installer":"*"
    },
    "extra": {
        "map": [
            ["app/code/local/Slv/Upsmi", "app/code/local/Slv/Upsmi"],
            ["app/etc/modules/Slv_Upsmi.xml", "app/etc/modules/Slv_Upsmi.xml"]
        ],
        "auto-append-gitignore": true
    }
}